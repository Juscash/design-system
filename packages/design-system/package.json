{
  "name": "@Juscash/design-system",
  "version": "0.1.0",
  "description": "Design system Juscash baseado em Ant Design 6",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist"
  },
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "dependencies": {
    "antd": "^6.0.0",
    "@ant-design/cssinjs": "^1.21.0",
    "lucide-react": "^0.468.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "rimraf": "^5.0.0",
    "@types/node": "^20.17.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}


