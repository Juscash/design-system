{
  "name": "@Juscash/design-system",
  "version": "0.1.22",
  "description": "Design system Juscash baseado em Ant Design 6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juscash/design-system.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.0",
    "antd": "^6.1.1",
    "lucide-react": "^0.468.0",
    "vite": "^7.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
